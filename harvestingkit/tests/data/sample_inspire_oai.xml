  <marc:datafield tag="245" ind1=" " ind2=" ">
    <marc:subfield code="a">High-precision gas gain and energy transfer measurements in Ar–CO2 mixtures</marc:subfield>
  </marc:datafield>
  <marc:datafield tag="300" ind1=" " ind2=" ">
